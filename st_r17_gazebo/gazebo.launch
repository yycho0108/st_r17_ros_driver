<launch>
    <include file="$(find gazebo_ros)/launch/empty_world.launch" />

    <param name="robot_description" textfile="$(find st_r17_description)/urdf/robot.urdf" />
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model st_r17 -param robot_description" />

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
        respawn="false" output="screen">
        <remap from="/joint_states" to="/st_r17/joint_states" />
    </node>

    <!-- For Gazebo? -->
    <node
        name="fake_joint_calibration"
        pkg="rostopic"
        type="rostopic"
        args="pub /calibrated std_msgs/Bool true" />
</launch>
